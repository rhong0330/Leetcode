class MyStack {
private:
    stack<int> mystack;
public:
    MyStack() {

    }
    
    void push(int x) {
        mystack.push(x);
    }
    
    int pop() {
        int popValue = mystack.top();
        mystack.pop();
        return popValue;
    }
    
    int top() {
        return mystack.top();
    }
    
    bool empty() {
        return mystack.empty();
    }
};

/**
 * Your MyStack object will be instantiated and called as such:
 * MyStack* obj = new MyStack();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->top();
 * bool param_4 = obj->empty();
 */
